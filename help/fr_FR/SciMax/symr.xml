          xmlns:db="http://docbook.org/ns/docbook">
  <info>
    <pubdate>$LastChangedDate$</pubdate>
  </info>
  
  <refnamediv>
    <refname>symr</refname>
    
    <refpurpose>Convertit un nombre en rationel</refpurpose>
  </refnamediv>

  <refsynopsisdiv>
    <title>Séquence d'appel</title>
    
    <synopsis>y=symr(x)</synopsis>
  </refsynopsisdiv>

  <refsection>
    <title>Paramètres</title>
    
    <variablelist>
      <varlistentry>
	<term>x</term>
	
	<listitem>
	  <para>peut être du type entier, double, complexe, chaîne, sym, polynôme</para>
	</listitem>
      </varlistentry>
      <varlistentry>
	<term>y</term>
	
	<listitem>
	  <para>est du type sym</para>
	</listitem>
      </varlistentry>
    </variablelist>
  </refsection>

  <refsection>
    <title>Description</title>
    
    <para>La fonction symr (r comme rationnel) convertit un nombre en rationnel. </para>
    
  </refsection>
  
  <refsection>
    <title>Exemples</title>
    
    <programlisting role="example">
symr(0.12345)
    </programlisting>
  </refsection>
  
  <refsection>
    <title>Auteur</title>
    
    <simplelist type="vert">
      <member>Calixte Denizet</member>
    </simplelist>
  </refsection>
  
  <refsection>
    <title>Voir aussi</title>
    
