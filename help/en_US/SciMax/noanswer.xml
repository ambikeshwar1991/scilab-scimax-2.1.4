  <info>
    <pubdate>$LastChangedDate$</pubdate>
  </info>
  
  <refnamediv>
    <refname>noanswer</refname>
    
    <refpurpose>Interrupts Maxima's question</refpurpose>
  </refnamediv>

  <refsynopsisdiv>
    <title>Calling Sequence</title>
    
    <synopsis>noanswer()</synopsis>
  </refsynopsisdiv>

  <refsection>
    <title>Description</title>
    
    <para>This function is used when you don't want to answer to a question asked by Maxima.</para>
    
  </refsection>
  
  <refsection>
    <title>Examples</title>
    
    <programlisting role="example">
Syms(['a' 'x'])
assume(a>1)
integrate(x^a/(x+1)^'5/2',x,0,%inf)
noanswer()
    </programlisting>
  </refsection>
  
  <refsection>
    <title>Author</title>
    
    <simplelist type="vert">
      <member>Calixte Denizet</member>
    </simplelist>
  </refsection>
  
